<actions>
    <action>
        <actionName>CUSTOM-liquibase-update</actionName>
        <displayName>liquibase-update</displayName>
        <goals>
            <goal>resources:resources</goal>
            <goal>liquibase:update</goal>
        </goals>
        <properties>
            <liquibase.propertyFile>src/main/resources/configLiquibase/liquibase.properties</liquibase.propertyFile>
        </properties>
        <activatedProfiles>
            <activatedProfile>development</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-GENERAR-SQL</actionName>
        <displayName>liquibase-generar-sql</displayName>
        <goals>
            <goal>resources:resources</goal>
            <goal>liquibase:updateSQL</goal>
        </goals>
        <properties>
            <liquibase.propertyFile>src/main/resources/configLiquibase/liquibase-generate-sql.properties</liquibase.propertyFile>
        </properties>            
        <activatedProfiles>
            <activatedProfile>development</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-Diferencias-base</actionName>
        <displayName>liquibase-generar-diferencias</displayName>
        <goals>
            <goal>resources:resources</goal>
            <goal>liquibase:diff</goal>
        </goals>
        <properties>
            <liquibase.propertyFile>src/main/resources/configLiquibase/liquibase-diferencias.properties</liquibase.propertyFile>
        </properties>            
        <activatedProfiles>
            <activatedProfile>development</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-liquibase-rollback</actionName>
        <displayName>liquibase-rollback</displayName>
        <goals>
            <goal>resources:resources</goal>
            <goal>liquibase:rollback</goal>
        </goals>
        <properties>
            <liquibase.rollbackCount>1</liquibase.rollbackCount>
        </properties>
        <activatedProfiles>
            <activatedProfile>development</activatedProfile>
        </activatedProfiles>
    </action>
        
        
        
        
        
        
