<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

    <application>
        <resource-bundle>
            <base-name>sv.gob.mined.siges.web.mensajes.EtiquetasMultipleResourceBundle</base-name>
            <var>msg</var>
        </resource-bundle>
    </application>
    
    <navigation-rule>
        <from-view-id>*</from-view-id>


 	<navigation-case>
		 <from-outcome>IR_A_ayudas</from-outcome>
		 <to-view-id>/pp/ayudas.xhtml</to-view-id>
		  <redirect/>
 	</navigation-case>
        
 	
 	<navigation-case>
		 <from-outcome>IR_A_operaciones</from-outcome>
		 <to-view-id>/pp/operaciones.xhtml</to-view-id>
		  <redirect/>
 	</navigation-case>
 	<navigation-case>
		 <from-outcome>IR_A_usuarios</from-outcome>
		 <to-view-id>/pp/usuarios.xhtml</to-view-id>
		  <redirect/>
 	</navigation-case>
 	<navigation-case>
		 <from-outcome>IR_A_roles</from-outcome>
		 <to-view-id>/pp/roles.xhtml</to-view-id>
		  <redirect/>
 	</navigation-case>
<!-- CASOS DE NAVEGACION CODIGUERA --> 





    </navigation-rule>
    

    
</faces-config>
